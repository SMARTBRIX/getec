<?xml version="1.0" encoding="UTF-8"?>
<odoo>
<record id="property_product_template_form_view" model="ir.ui.view">
    <field name="name">property.product.template.form</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_form_view"/>
    <field name="arch" type="xml">
    	<xpath expr="//page[@name='general_information']/group/group[@name='group_general']" position="inside">
			<field name="is_rent"/>
            <field name="is_ancillary_costs"/>
            <field name="is_heating_costs"/>
    	</xpath>
    </field>
</record>
</odoo>