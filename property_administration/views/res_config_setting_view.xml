<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="res_config_settings_form_property_view" model="ir.ui.view">
	    <field name="name">res.config.settings.property.form</field>
	    <field name="model">res.config.settings</field>
	    <field name="inherit_id" ref="account.res_config_settings_view_form"/>
	    <field name="arch" type="xml">
	    	<xpath expr="//div[@id='default_accounts']" position="inside">
				<div class="content-group">
	        		<div class="row mt8">
	                    <label for="charges_account_id" class="col-lg-3 o_light_label"/>
	                    <field name="charges_account_id"/>
	                </div>
	            </div>
			</xpath>
	    </field>
    </record>
</odoo>